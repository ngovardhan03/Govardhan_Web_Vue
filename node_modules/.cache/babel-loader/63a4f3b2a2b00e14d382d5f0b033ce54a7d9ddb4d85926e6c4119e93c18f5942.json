{"ast":null,"code":"import MyHeader from './components/MyHeader.vue';\nimport BookDetails from './components/BookDetails.vue';\nimport MyFooter from './components/MyFooter.vue';\nexport default {\n  name: 'App',\n  components: {\n    MyHeader,\n    BookDetails,\n    MyFooter\n  },\n  data() {\n    return {\n      Title: 'Electronics Gallery',\n      ProductDetails: [],\n      numberofoverduebooks: new Date().getMonth(),\n      loading: true,\n      error: null\n    };\n  },\n  methods: {\n    async fetchdetails() {\n      try {\n        const response = await fetch('https://govardhan-web-node.onrender.com/api');\n        if (!response.ok) {\n          throw new Error('Network response was not ok');\n        }\n        const data = await response.json();\n        this.ProductDetails = data;\n        console.log('Data fetched:', data);\n      } catch (error) {\n        console.error('Error fetching news feed:', error.message);\n        this.error = 'Error fetching data. Please try again later.';\n      } finally {\n        this.loading = false;\n      }\n    }\n  },\n  async created() {\n    await this.fetchdetails();\n  }\n};","map":{"version":3,"names":["MyHeader","BookDetails","MyFooter","name","components","data","Title","ProductDetails","numberofoverduebooks","Date","getMonth","loading","error","methods","fetchdetails","response","fetch","ok","Error","json","console","log","message","created"],"sources":["C:\\Users\\HARSHA\\Downloads\\librarydata\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <my-header :UniversityName=\"Title\" />\n    <book-details :bookdetails=\"ProductDetails\" />\n    <my-footer :countbooks=\"numberofoverduebooks\" />\n    <div v-if=\"loading\">Loading...</div>\n    \n  </div>\n</template>\n\n<script>\nimport MyHeader from './components/MyHeader.vue';\nimport BookDetails from './components/BookDetails.vue';\nimport MyFooter from './components/MyFooter.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    MyHeader,\n    BookDetails,\n    MyFooter,\n  },\n  data() {\n    return {\n      Title: 'Electronics Gallery',\n      ProductDetails: [],\n      numberofoverduebooks: new Date().getMonth(),\n      loading: true,\n      error: null,\n    };\n  },\n  methods: {\n    async fetchdetails() {\n      try {\n        const response = await fetch('https://govardhan-web-node.onrender.com/api');\n        if (!response.ok) {\n          throw new Error('Network response was not ok');\n        }\n        const data = await response.json();\n        this.ProductDetails = data;\n        console.log('Data fetched:', data);\n      } catch (error) {\n        console.error('Error fetching news feed:', error.message);\n        this.error = 'Error fetching data. Please try again later.';\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n  async created() {\n    await this.fetchdetails();\n  },\n};\n</script>\n\n<style>\nbody {\n  font-family: 'Montserrat', sans-serif;\n  margin: 0;\n  padding: 0;\n}\n\n#app {\n  text-align: center;\n  margin-top: 20px;\n}\n\n#app div {\n  margin-top: 10px;\n  font-weight: bold;\n  color: black;\n}\n\n.loading {\n  color: #00f;\n}\n\n.error {\n  color: #f00;\n}\n</style>"],"mappings":"AAWA,OAAOA,QAAO,MAAO,2BAA2B;AAChD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,QAAO,MAAO,2BAA2B;AAEhD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,QAAQ;IACRC,WAAW;IACXC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,qBAAqB;MAC5BC,cAAc,EAAE,EAAE;MAClBC,oBAAoB,EAAE,IAAIC,IAAI,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC;MAC3CC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,6CAA6C,CAAC;QAC3E,IAAI,CAACD,QAAQ,CAACE,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,6BAA6B,CAAC;QAChD;QACA,MAAMb,IAAG,GAAI,MAAMU,QAAQ,CAACI,IAAI,CAAC,CAAC;QAClC,IAAI,CAACZ,cAAa,GAAIF,IAAI;QAC1Be,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEhB,IAAI,CAAC;MACpC,EAAE,OAAOO,KAAK,EAAE;QACdQ,OAAO,CAACR,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CA